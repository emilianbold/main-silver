<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="netbeans" name="dlight.remote.impl">
    <description>Builds, tests, and runs the project org.netbeans.modules.dlight.remote.impl</description>
    <import file="../nbbuild/templates/projectized.xml"/>
    <target name="-overwrite-fs_server-zip" if="${zip.binaries.from.release.bin}">
        <property name="binaries.dir" value="release/bin"/>
        <property name="binaries.zip" value="external/fs_server-1.0.zip"/>
        <echo message="Updating external/fs_server-1.0.zip with binaries from release/bin"></echo>
        <zip basedir="${binaries.dir}" destfile="${binaries.zip}"/>
    </target>
    <target name="-release.files" depends="projectized-common.-release.files,-overwrite-fs_server-zip">
    </target>
</project>
