<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Window">
            <file name="demo-rest-explorer-ExplorerAction.instance">
                <attr name="component" methodvalue="demo.rest.explorer.ExplorerTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="demo.rest.explorer.Bundle#CTL_ExplorerAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
            <file name="demo-rest-editor-MsgDetailsAction.instance">
                <attr name="component" methodvalue="demo.rest.editor.MsgDetailsTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="demo.rest.editor.Bundle#CTL_MsgDetailsAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
        </folder>
    </folder>
    <folder name="Menu">
        <folder name="Window">
            <file name="ExplorerAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/demo-rest-explorer-ExplorerAction.instance"/>
            </file>
            <file name="MsgDetailsAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Window/demo-rest-editor-MsgDetailsAction.instance"/>
            </file>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="File">
            <file name="org-openide-actions-SaveAction.shadow">
                <attr name="originalFile" stringvalue="Actions/System/org-openide-actions-SaveAction.instance"/>
                <attr name="position" intvalue="444"/>
            </file>
            <file name="org-openide-actions-SaveAllAction.shadow_hidden"/>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="ExplorerTopComponent.settings" url="explorer/ExplorerTopComponentSettings.xml"/>
            <file name="MsgDetailsTopComponent.settings" url="editor/MsgDetailsTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="MsgDetailsTopComponent.wstcref" url="editor/MsgDetailsTopComponentWstcref.xml"/>
            </folder>
            <folder name="explorer">
                <file name="ExplorerTopComponent.wstcref" url="explorer/ExplorerTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>
</filesystem>
