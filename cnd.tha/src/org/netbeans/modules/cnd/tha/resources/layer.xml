<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="DLight">
        <folder name="Configurations">
            <folder name="THA">
                <attr name="displayedName" stringvalue="Thread Profiling"/>
                <attr name="hidden" boolvalue="true"/>
                <attr name="system" boolvalue="true"/>
                <attr name="platforms" stringvalue="Solaris,Linux"/>
                <attr name="collector.providers" stringvalue="SunStudio"/>
                <attr name="indicator.providers" stringvalue="ProcFSReader,ProcReader,prstat,SunStudio"/>
                <folder name="KnownToolsConfigurationProviders">
                    <file name="DLightTHAToolConfigurationProvider.shadow">
                        <attr name="originalFile" stringvalue="DLight/ToolConfigurationProviders/DLightTHAToolConfigurationProvider.instance"/>
                    </file>
                    <file name="SyncToolConfigurationProvider.shadow">
                        <attr name="originalFile" stringvalue="DLight/ToolConfigurationProviders/SyncToolConfigurationProvider.instance"/>
                        <attr name="detailsEnabled" boolvalue="false"/>
                    </file>
                </folder>
                <folder name="ConfigurationOptions">
                    <file name="THAConfigurationOptions.instance">
                        <attr name="instanceClass" stringvalue="org.netbeans.modules.cnd.tha.THAConfigurationOptions"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Actions">
        <folder name ="Window">
            <file name="org-netbeans-modules-cnd-tha-ui-THAIndicatorTopComponentActivateAction.instance">
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
                <attr name="component" methodvalue="org.netbeans.modules.cnd.tha.ui.THAIndicatorsTopComponent.activateInstance"/>
                <attr name="displayName" bundlevalue="org.netbeans.modules.cnd.tha.ui.Bundle#CTL_DLightIndicatorsTopComponent"/>
                <attr name="noIconInMenu" boolvalue="true"/>
            </file>
        </folder>
        <folder name="Profile">
            <file name="org-netbeans-modules-cnd-tha-actions-THAMainProjectAction.instance"/>
        </folder>
    </folder>
    <folder name="Windows2">
        <folder name="Components">
            <file name="THAIndicatorsTopComponent.settings" url="THAIndicatorsTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="explorer">
                <file name="THAIndicatorsTopComponent.wstcref" url="THAIndicatorsTopComponentWstcref.xml">
                    <attr name="position" intvalue="2500"/>
                </file>
            </folder>
        </folder>
    </folder>
    <folder name="Toolbars">
        <folder name="Build">
            <file name="org-netbeans-modules-cnd-tha-actions-THAMainProjectAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Profile/org-netbeans-modules-cnd-tha-actions-THAMainProjectAction.instance"/>
                <attr name="position" intvalue="500"/>
            </file>
        </folder>
    </folder>

  <folder name="Shortcuts">
        <file name="A-S-S.shadow">
            <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-cnd-tha-ui-THAIndicatorTopComponentActivateAction.instance"/>
        </file>
        <file name="C-A-S-D.shadow">
            <attr name="originalFile" stringvalue="Actions/Profile/org-netbeans-modules-cnd-tha-actions-THAMainProjectAction.instance"/>
        </file>
    </folder>
</filesystem>
