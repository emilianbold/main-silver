<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="DLight">
        <folder name="Configurations">
            <folder name="THA">
                <folder name="KnownToolsConfigurationProviders">
                    <file name="DLightTHAToolConfigurationProvider.shadow">
                        <attr name="originalFile" stringvalue="DLight/ToolConfigurationProviders/DLightTHAToolConfigurationProvider.instance"/>
                    </file>
                    <file name="SyncToolConfigurationProvider.shadow">
                        <attr name="originalFile" stringvalue="DLight/ToolConfigurationProviders/SyncToolConfigurationProvider.instance"/>
                    </file>
                </folder>
                <folder name="ConfigurationOptions">
                    <file name="THAConfigurationOptions.instance">
                        <attr name="instanceClass" stringvalue="org.netbeans.modules.cnd.tha.THAConfigurationOptions"/>
                    </file>
                </folder>
            </folder>
        </folder>
    </folder>
    <folder name="Actions">
        <folder name ="Window">
            <file name="org-netbeans-modules-cnd-tha-ui-THAIndicatorTopComponentActivateAction.instance">
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
                <attr name="component" methodvalue="org.netbeans.modules.cnd.tha.ui.THAndicatorsTopComponent.activateInstance"/>
                <attr name="displayName" bundlevalue="org.netbeans.modules.cnd.tha.ui.Bundle#CTL_DLightIndicatorsTopComponent"/>
                <attr name="noIconInMenu" boolvalue="true"/>
            </file>
        </folder>
        <folder name="Profile">
            <file name="org-netbeans-modules-cnd-tha-actions-THAToolbarDropdownAction.instance"/>
        </folder>
    </folder>

    <folder name="Toolbars">
        <folder name="Build">
            <file name="org-netbeans-modules-cnd-tha-actions-THAToolbarDropdownAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Profile/org-netbeans-modules-cnd-tha-actions-THAToolbarDropdownAction.instance"/>
                <attr name="position" intvalue="500"/>
            </file>
        </folder>
    </folder>

</filesystem>
