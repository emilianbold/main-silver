<?xml version="1.0" encoding="UTF-8"?>
<project name="javax.activation" default="netbeans" basedir=".">
    <import file="../nbbuild/templates/projectized.xml"/>
    <target name="jar">
        <jar jarfile="${cluster}/${module.jar}">
            <zipfileset src="external/geronimo-activation-1.1-spec-1.0.jar"/>
            <zipfileset src="external/geronimo-activation-2.0.2.jar"/>
            <manifest>
                <attribute name="Bundle-SymbolicName" value="javax.activation"/>
                <attribute name="Bundle-Version" value="1.1.0"/>
                <attribute name="Export-Package" value="javax.activation; vendor=geronimo,org.apache.geronimo.activation.handlers;x-internal:=true"/>
            </manifest>
        </jar>
    </target>
</project>

