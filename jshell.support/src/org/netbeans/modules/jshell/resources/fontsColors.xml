<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE fontscolors PUBLIC "-//NetBeans//DTD Editor Fonts and Colors settings 1.1//EN" "http://www.netbeans.org/dtds/EditorFontsColors-1_1.dtd">

<fontscolors>
    <fontcolor name="prompt" default="identifier">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="delimiter" default="string" bgColor="ccffcc"/>
    <fontcolor name="message" default="string" bgColor="ccffcc"/>
    <fontcolor name="output" default="string" bgColor="ffffcc"/>
    <fontcolor name="java" foreColor="000000"/>
    <fontcolor name="mark-error" default="error"/>
    <fontcolor name="command" default="identifier">
        <font style="bold"/>
    </fontcolor>
    <fontcolor name="command-text" default="text"/>
</fontscolors>
