<?xml version="1.0" encoding="UTF-8"?>
<project basedir="." default="netbeans" name="findbugs">
    <description>Builds, tests, and runs the project org.netbeans.modules.findbugs</description>
    <import file="../nbbuild/templates/projectized.xml"/>
    <target name="files-init" depends="projectized-common.files-init,download-findbugs"/>
    <target name="download-findbugs">
        <ant dir="../libs.findbugs" target="download-findbugs" inheritall="false" inheritrefs="false">
            <property name="external.dir" location="build/external" />
            <property name="download.findbugs" value="true" />
        </ant>
    </target>
</project>
