
var |>GLOBAL:areas<| = [];
var |>GLOBAL:languages<| = [];
var |>GLOBAL:components<| = [];

  
function |>METHOD:countComponentTables<|() { 
    |>GLOBAL:$<|(".sortable").each(function() { // each table
        var index = 0;
        var sums = new Array();
        var id = |>GLOBAL:$<|(this).attr("id");
        |>GLOBAL:$<|(this).find("thead th").each(function() {
            index++;
            if (index > 1 && (!|>GLOBAL:$<|(this).attr("id") || |>GLOBAL:$<|(this).attr("id").indexOf("dummy") > -1))
                sums.push(|>GLOBAL:sumOfColumns<|(id, index, true));

        });

        var tfoot = "<tfoot><tr class=\"bug_total_row\"><td>Total</td>";
        for (var i = 0; i < sums.length; i++) {
            tfoot += "<td>" + sums[i] + "</td>";
        }

        |>GLOBAL:$<|(this).append(tfoot + "</tr></tfoot>");


    });


}

