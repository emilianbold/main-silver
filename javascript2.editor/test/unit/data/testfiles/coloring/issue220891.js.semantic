var |>GLOBAL:hiddenComponents<| = true;

function |>METHOD:toggleComponents<|() {
    if (|>GLOBAL:hiddenComponents<|) {
        $("tr[data-hidden=\"1\"]").css("display", "table-row");
        |>GLOBAL:hiddenComponents<| = false;
    }
    else {
        $("tr[data-hidden=\"1\"]").css("display", "none");
        |>GLOBAL:hiddenComponents<| = true;
    }
}