<project name="org.netbeans.modules.persistencelibrary" default="netbeans" basedir=".">
    <description>Builds, tests, and runs the project org.netbeans.modules.persistencelibrary.</description>
    <import file="nbproject/build-impl.xml"/>
    <target name="netbeans-extra">
        <property name="external_dir" value="external"/>
        <copy todir="${cluster}/modules/ext/persistence/" failonerror="false" overwrite="true" verbose="true">
            <fileset dir="${external_dir}">
                <include name="**/*.jar"/>
            </fileset>
        </copy>
    </target>
</project>
