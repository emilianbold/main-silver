<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="OptionsExport">
        <folder name="PHP">
            <file name="Zend">
                <attr name="include" stringvalue="config/Preferences/org/netbeans/modules/php/zend/zend.properties"/>
                <attr name="displayName" bundlevalue="org.netbeans.modules.php.zend.resources.Bundle#LBL_ZendExportName"/>
                <attr name="position" intvalue="400"/>
            </file>
        </folder>
    </folder>
    <folder name="PHP">
        <folder name="Frameworks">
            <file name="org-netbeans-modules-php-zend-ZendPhpFrameworkProvider.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.php.zend.ZendPhpFrameworkProvider.getInstance"/>
                <attr name="position" intvalue="200"/>
            </file>
        </folder>
    </folder>
    <folder name="Actions">
        <folder name="PHP">
            <file name="org-netbeans-modules-php-zend-ui-actions-RunCommandAction.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.php.zend.ui.actions.RunCommandAction.getInstance"/>
            </file>
        </folder>
    </folder>
    <folder name="Editors">
        <folder name="text">
            <folder name="x-php5">
                <folder name="Actions">
                    <!-- do not move under Actions/PHP because it is ContextAware action! -->
                    <file name="org-netbeans-modules-php-zend-ui-actions-GoToActionOrViewAction.instance">
                        <attr name="instanceCreate" methodvalue="org.netbeans.modules.php.zend.ui.actions.GoToActionOrViewAction.getInstance"/>
                    </file>
                </folder>
                <folder name="Popup">
                    <folder name="goto">
                        <file name="org-netbeans-modules-php-zend-ui-actions-GoToActionOrViewAction.shadow">
                            <attr name="originalFile" stringvalue="Editors/text/x-php5/Actions/org-netbeans-modules-php-zend-ui-actions-GoToActionOrViewAction.instance"/>
                            <attr name="position" intvalue="110"/>
                        </file>
                    </folder>
                </folder>
            </folder>
        </folder>
    </folder>
</filesystem>
