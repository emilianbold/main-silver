<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>

    <folder name="Actions">
        <folder name="Versioning">
            <file name="org-netbeans-modules-bugtracking-ui-query-QueryAction.instance"/>
            <file name="org-netbeans-modules-bugtracking-ui-issue-IssueAction.instance"/>
        </folder>
        <folder name="Window">
            <file name="org-netbeans-modules-bugtracking-ui-query-QueryAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.bugtracking.Bundle"/>
                <attr name="component" methodvalue="org.netbeans.modules.bugtracking.ui.query.QueryTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.netbeans.modules.bugtracking.ui.query.Bundle#CTL_QueryAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
            <!-- XXX no findInstance method:
            <file name="org-netbeans-modules-bugtracking-ui-issue-IssueAction.instance">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.netbeans.modules.bugtracking.Bundle"/>
                <attr name="component" methodvalue="org.netbeans.modules.bugtracking.ui.issue.IssueTopComponent.findInstance"/>
                <attr name="displayName" bundlevalue="org.netbeans.modules.bugtracking.ui.issue.Bundle#CTL_IssueAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.windows.TopComponent.openAction"/>
            </file>
            -->
        </folder>
    </folder>

    <folder name="Menu">
        <folder name="Versioning">
            <file name="org-netbeans-modules-bugtracking-ui-query-QueryAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Versioning/org-netbeans-modules-bugtracking-ui-query-QueryAction.instance"/>
                <attr name="position" intvalue="200"/>
            </file>
            <file name="org-netbeans-modules-bugtracking-ui-query-IssueAction.shadow">
                <attr name="originalFile" stringvalue="Actions/Versioning/org-netbeans-modules-bugtracking-ui-issue-IssueAction.instance"/>
                <attr name="position" intvalue="300"/>
            </file>
        </folder>
    </folder>
    
    <folder name="Windows2">
        <folder name="Components">
            <file name="QueryTopComponent.settings" url="ui/query/QueryTopComponentSettings.xml"/>
        </folder>
        <folder name="Modes">
            <folder name="editor">
                <file name="QueryTopComponent.wstcref" url="ui/query/QueryTopComponentWstcref.xml"/>
            </folder>
        </folder>
    </folder>

    <folder name="TaskList">
        <folder name="Scanners">
            <file name="TaskListProvider.instance">
                <attr name="instanceOf" stringvalue="org.netbeans.spi.tasklist.PushTaskScanner"/>
                <attr name="instanceCreate" methodvalue="org.netbeans.modules.bugtracking.tasklist.TaskListProvider.getInstance"/>
            </file>
        </folder>
        <folder name="Groups">
            <file name="issue.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.tasklist.Task.createGroup"/>
                <attr name="localizingBundle" stringvalue="org.netbeans.modules.bugtracking.tasklist.Bundle"/>
                <attr name="groupName" stringvalue="nb-tasklist-issue"/>
                <attr name="diplayNameKey" stringvalue="LBL_issue_group"/>
                <attr name="descriptionKey" stringvalue="HINT_issue_group"/>
                <attr name="iconKey" stringvalue="ICON_issue"/>
                <attr name="position" intvalue="600"/>
            </file>
        </folder>
    </folder>
</filesystem>
