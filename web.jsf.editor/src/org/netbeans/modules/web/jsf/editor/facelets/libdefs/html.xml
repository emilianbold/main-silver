<library id="http://java.sun.com/jsf/html">
    <common_attributes>
        <attr name="dir">
            <type name="enum">
                <val>LTR</val>
                <val>RTL</val>
            </type>
        </attr>
        <attr name="rendered">
            <type name="boolean"/>
        </attr>
        <attr name="disabled">
            <type name="boolean"/>
        </attr>
        <attr name="readonly">
            <type name="boolean"/>
        </attr>
        <attr name="required">
            <type name="boolean"/>
        </attr>
        <attr name="escape">
            <type name="boolean"/>
        </attr>
        <attr name="hideNoSelectionOption">
            <type name="boolean"/>
        </attr>
    </common_attributes>
    <tag name="message">
        <attr name="showDetail">
            <type name="boolean"/>
        </attr>
        <attr name="showSummary">
            <type name="boolean"/>
        </attr>
    </tag>
    <tag name="messages">
        <attr name="globalOnly">
            <type name="boolean"/>
        </attr>
        <attr name="tooltip">
            <type name="boolean"/>
        </attr>
        <attr name="layout">
            <type name="enum">
                <val>table</val>
                <val>list</val>
            </type>
        </attr>
    </tag>
    <tag name="panelGroup">
        <attr name="layout">
            <type name="enum">
                <val>block</val>
                <val>span</val>
            </type>
        </attr>
    </tag>
    <tag name="dataTable">
        <attr name="frame">
            <type name="enum">
                <val>none</val>
                <val>above</val>
                <val>below</val>
                <val>hsides</val>
                <val>vsides</val>
                <val>hsides</val>
                <val>rhs</val>
                <val>lhs</val>
                <val>border</val>
            </type>
        </attr>
    </tag>
    <tag name="selectManyCheckbox">
        <attr name="layout">
            <type name="enum">
                <val>pageDirection</val>
                <val>lineDirection</val>
            </type>
        </attr>
    </tag>
</library>